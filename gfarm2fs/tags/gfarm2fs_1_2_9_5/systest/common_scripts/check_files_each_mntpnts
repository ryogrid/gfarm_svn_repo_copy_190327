#
# files check with md5
#
check_files_each_mntpnts()
{
	HOSTID=$1
	MNTDIR_PREFIX=$2
	COUNT=$3
	TARGET_DIR=$4
	NFILES=$5
	CHKSUM=$6

	$GFSERVICE systest::gfarm2fs::check_files_n_each_mntpnts $HOSTID $MNTDIR_PREFIX \
		$COUNT $TARGET_DIR $NFILES $CHKSUM
	if [ $? -ne 0 ]; then
		log_warn "gfservice check_files_n_each_mntpnts failed" \
			"$HOSTID $CHKSUM"
		return 1
	fi

	return 0
}
