          <url></url>
          <status>0</status>
          <priority>4</priority>
          <comments></comments>
        </trigger>
        <trigger>
          <description>Low free disk space on {HOSTNAME} volume /</description>
          <type>0</type>
          <expression>{Template_Gfarm_zabbix:vfs.fs.size[/,pfree].last(0)}&lt;10</expression>
          <url></url>
          <status>0</status>
          <priority>2</priority>
          <comments></comments>
        </trigger>
        <trigger>
